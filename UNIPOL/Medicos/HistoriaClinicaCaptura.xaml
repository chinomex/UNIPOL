<Window x:Class="UNIPOL.Medicos.HistoriaClinicaCaptura"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:UNIPOL.Medicos"
        mc:Ignorable="d"
        Title="Historia Clinica Captura" 
        Height="720" 
        Width="1000"
        
        WindowStartupLocation="CenterScreen"
        WindowState="Normal"
        WindowStyle="SingleBorderWindow"
        ResizeMode="NoResize">
    <Grid>
        <materialDesign:Card>
            <TabControl HorizontalContentAlignment="Left"
                        Style="{StaticResource MaterialDesignFilledTabControl}"
                        SelectionChanged="tbHistoria_SelectionChanged"
                        Name="tbHistoria">
                <TabItem Header="Datos del cadete">
                    <StackPanel Margin="10,10,15,10"
                                Orientation="Vertical"
                                VerticalAlignment="Top"
                                Height="570">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="150"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="150"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="150"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>                                
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                                <RowDefinition Height="57"></RowDefinition>
                            </Grid.RowDefinitions>


                            <TextBlock Grid.Row="0"
                                       Grid.Column="0"
                                       Text="Nombre"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       Margin="0,0,0,5"
                                       FontSize="16"
                                       Foreground="#9f9f9f"></TextBlock>
                            <StackPanel Orientation="Horizontal"
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="5"
                                        Margin="10,0,0,0">
                                <TextBox Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                         VerticalAlignment="Center"
                                         x:Name="txtPaciente"
                                         Padding="0,5,0,5"
                                         Width="740"
                                         FontSize="16"
                                         Text="{Binding datosPaciente.Nombre, UpdateSourceTrigger=PropertyChanged}"
                                         IsEnabled="False"/>
                                <Button Content="{materialDesign:PackIcon AccountSearch}"
                                    VerticalAlignment="Bottom"
                                    FontSize="34"
                                    Foreground="#fff"
                                    Margin="10,0,0,0"
                                    x:Name="btnBuscarPaciente"
                                    Click="btnBuscarPaciente_Click"></Button>
                            </StackPanel>



                            <TextBlock Grid.Row="1"
                                       Grid.Column="0"
                                       Text="Edad (años)"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="1"
                                       Text="{Binding datosPaciente.EdadCompleta, UpdateSourceTrigger=PropertyChanged}"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Left"
                                       FontSize="16"
                                       Margin="10,0,0,0"></TextBlock>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="2"
                                       Text="Fecha de nacimiento"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Foreground="#9f9f9f"
                                       TextWrapping="Wrap"></TextBlock>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="3"
                                       Text="{Binding datosPaciente.FechaNacimiento, UpdateSourceTrigger=PropertyChanged, StringFormat='dd/MM/yyy'}"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Left"
                                       FontSize="16"
                                       Margin="10,0,0,0"></TextBlock>

                            <TextBlock Grid.Row="1"
                                       Grid.Column="4"
                                       Text="Telefono"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="5"
                                       Text="{Binding datosPaciente.Telefono, UpdateSourceTrigger=PropertyChanged}"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Left"
                                       FontSize="16"
                                       Margin="10,0,0,0"></TextBlock>


                            <TextBlock Grid.Row="2"
                                       Grid.Column="0"
                                       Text="Modalidad"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="2" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtModalidad"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="40"/>




                            <TextBlock Grid.Row="3"
                                       Grid.Column="0"
                                       Text="Estado Civil"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <ComboBox Grid.Row="3"
                                      Grid.Column="1"
                                      Grid.ColumnSpan="2"
                                      VerticalAlignment="Bottom"
                                      FontSize="16"
                                      Margin="10,0,0,0"
                                      ItemsSource="{Binding lstEstadosCiviles, UpdateSourceTrigger=PropertyChanged}"
                                      SelectedItem="{Binding EstadoCivilSeleccionado, UpdateSourceTrigger=PropertyChanged}"
                                      SelectedValue="Codigo"
                                      DisplayMemberPath="Descripcion">
                            </ComboBox>


                            <TextBlock Grid.Row="3"
                                       Grid.Column="3"
                                       Text="Sexo"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBlock Grid.Row="3"
                                       Grid.Column="4"
                                       Grid.ColumnSpan="2"
                                       Text="{Binding datosPaciente.Genero, UpdateSourceTrigger=PropertyChanged}"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Left"
                                       FontSize="16"
                                       Margin="10,0,0,5"></TextBlock>                            
                            <!--<ComboBox Grid.Row="3"
                                      Grid.Column="4"
                                      Grid.ColumnSpan="2"
                                      VerticalAlignment="Bottom"
                                      FontSize="16"
                                      Margin="10,0,0,0"
                                      ItemsSource="{Binding lstGenero, UpdateSourceTrigger=PropertyChanged}"
                                      SelectedItem="{Binding GeneroSeleccionado, UpdateSourceTrigger=PropertyChanged}"
                                      SelectedValue="Codigo"
                                      DisplayMemberPath="Descripcion">
                            </ComboBox>-->



                            <TextBlock Grid.Row="4"
                                       Grid.Column="0"
                                       Text="Señas particulares"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="4" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtSParticulares"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="300"/>
                           

                            <TextBlock Grid.Row="5"
                                       Grid.Column="0"
                                       Text="Escolaridad"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="5" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtEscolaridad"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="100"/>


                            <TextBlock Grid.Row="6"
                                       Grid.Column="0"
                                       Text="Originario"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="6" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtOriginario"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="100"/>


                            <TextBlock Grid.Row="7"
                                       Grid.Column="0"
                                       Text="Domicilio"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="7" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtDomicilio"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="250"/>


                            <TextBlock Grid.Row="8"
                                       Grid.Column="0"
                                       Text="Procedencia"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="8" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtProcedencia"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="300"/>

                            <TextBlock Grid.Row="9"
                                       Grid.Column="0"
                                       Text="Trabajo"
                                       VerticalAlignment="Bottom"
                                       HorizontalAlignment="Right"
                                       FontSize="16"
                                       Margin="5"
                                       Foreground="#9f9f9f"></TextBlock>
                            <TextBox Grid.Row="9" 
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                     VerticalAlignment="Center"
                                     x:Name="txtTrabajo"
                                     Padding="0,5,0,5"
                                     Margin="10,0,0,0"
                                     FontSize="16"
                                     MaxLength="100"/>


                        </Grid>
                    </StackPanel>
                </TabItem>
                <TabItem Header="Antecedentes">
                    <StackPanel Margin="10"
                                Orientation="Vertical"
                                VerticalAlignment="Top"
                                Height="570">
                        <ScrollViewer HorizontalScrollBarVisibility="Disabled"
                                      VerticalScrollBarVisibility="Auto"
                                      Height="570">
                            <Grid Margin="0,10,10,20">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25"></RowDefinition>
                                    <RowDefinition Height="80"></RowDefinition>
                                    <RowDefinition Height="20"></RowDefinition>
                                    <RowDefinition Height="80"></RowDefinition>
                                    <RowDefinition Height="20"></RowDefinition>
                                    <RowDefinition Height="420"></RowDefinition>
                                    <RowDefinition Height="20"></RowDefinition>
                                    <RowDefinition Height="397"></RowDefinition>
                                    <RowDefinition Height="20"></RowDefinition>
                                    <RowDefinition Height="80"></RowDefinition>
                                </Grid.RowDefinitions>


                                <TextBlock Grid.Row="0"
                                           Text="Antecedentes Heredo Familiares"
                                           FontSize="14"
                                           TextAlignment="Center"
                                           Foreground="#000"
                                           Background="Silver"
                                           Padding="3"></TextBlock>
                                <TextBox Grid.Row="1"
                                         TextWrapping="Wrap"
                                         VerticalAlignment="Stretch"
                                         VerticalContentAlignment="Bottom"
                                         MinWidth="100"
                                         AcceptsReturn="True"
                                         VerticalScrollBarVisibility="Auto"
                                         SpellCheck.IsEnabled="True"
                                         BorderBrush="Silver"
                                         BorderThickness="1"
                                         Text="" 
                                         Margin="0,0,0,20"
                                         Padding="5"
                                         MaxLength="450"
                                         x:Name="txtHeredoFamiliares"/>


                                <TextBlock Grid.Row="2"
                                           Text="Antecedentes Personales No Patológicos"
                                           FontSize="14"
                                           TextAlignment="Center"
                                           Foreground="#000"
                                           Background="Silver">
                                </TextBlock>
                                <TextBox Grid.Row="3"
                                         TextWrapping="Wrap"
                                         VerticalAlignment="Stretch"
                                         VerticalContentAlignment="Bottom"
                                         MinWidth="100"
                                         AcceptsReturn="True"
                                         VerticalScrollBarVisibility="Auto"
                                         SpellCheck.IsEnabled="True"
                                         BorderBrush="Silver"
                                         BorderThickness="1"
                                         Text="" 
                                         Margin="0,0,0,20"
                                         Padding="5"
                                         MaxLength="450"
                                         x:Name="txtPersonalesPatologicos"/>


                                <TextBlock Grid.Row="4"
                                           Text="Antecedentes Personales Patológicos"
                                           FontSize="14"
                                           TextAlignment="Center"
                                           Foreground="#000"
                                           Background="Silver"></TextBlock>
                                <StackPanel Grid.Row="5"
                                            Orientation="Vertical">
                                    <Grid >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="200"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="0"
                                                   Text="ANTECEDENTES CRONICOS DEGENERATIVOS"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="0" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="250"
                                                 x:Name="txtCronicoDegenerativos"/>


                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="0"
                                                   Text="ANTECEDENTES QUIRURGICOS"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="1" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="250"
                                                 x:Name="txtQuirurgicos"/>

                                        <TextBlock Grid.Row="2"
                                                   Grid.Column="0"
                                                   Text="ANTECEDENTES TRAUMATOLOGICOS"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="2" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="250"
                                                 x:Name="txtTraumatologicos"/>

                                        <TextBlock Grid.Row="3"
                                                   Grid.Column="0"
                                                   Text="TRANSFUSIONES"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="3" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="150"
                                                 x:Name="txtTransfusiones"/>


                                        <TextBlock Grid.Row="4"
                                                   Grid.Column="0"
                                                   Text="INFECTOCONTAGIOSOS"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="4" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="250"
                                                 x:Name="txtInfectocontagiosos"/>

                                        <TextBlock Grid.Row="5"
                                                   Grid.Column="0"
                                                   Text="HOSPITALIZACIONES"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="5" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="250"
                                                 x:Name="txtHozpitalizaciones"/>

                                        <TextBlock Grid.Row="6"
                                                   Grid.Column="0"
                                                   Text="COVID 19"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <TextBox Grid.Row="6" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 MaxLength="250"
                                                 x:Name="txtCovid"/>
                                    </Grid>
                                </StackPanel>





                                <TextBlock Grid.Row="6"
                                           Text="Antecedentes Gineco Obstetricos"
                                           FontSize="14"
                                           TextAlignment="Center"
                                           Foreground="#000"
                                           Background="Silver"></TextBlock>
                                <StackPanel Grid.Row="7"
                                            Orientation="Vertical">
                                    <StackPanel.Style>
                                        <Style TargetType="StackPanel">
                                            <Setter Property="IsEnabled" Value="True"></Setter>
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding datosPaciente.IdGenero, UpdateSourceTrigger=PropertyChanged}" Value="1">
                                                    <Setter Property="IsEnabled" Value="False"></Setter>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </StackPanel.Style>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="100"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="120"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="120"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="57"></RowDefinition>
                                            <RowDefinition Height="100"></RowDefinition>
                                            <RowDefinition Height="100"></RowDefinition>
                                        </Grid.RowDefinitions>



                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="0"
                                                   Text="MENARCA"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,10"></TextBlock>
                                        <StackPanel Grid.Row="0"
                                                    Grid.Column="1" 
                                                    Orientation="Horizontal">
                                            <TextBox Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                     VerticalAlignment="Center"
                                                     Padding="0,5,0,5"
                                                     Margin="10,0,0,0"
                                                     Width="151"
                                                     FontSize="16"
                                                     x:Name="txtMenarca"
                                                     PreviewKeyDown="txtMenarca_PreviewKeyDown"
                                                  >
                                            </TextBox>
                                            <TextBlock Text="AÑOS"
                                                       VerticalAlignment="Bottom"
                                                       Foreground="#9f9f9f"
                                                       Margin="5,5,5,10"
                                                       ></TextBlock>
                                        </StackPanel>


                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="2"
                                                   Text="EMBARAZO"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,10"></TextBlock>
                                        <TextBox Grid.Row="0" 
                                                 Grid.Column="3"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 FontSize="16"
                                                 x:Name="txtEmbarazo"
                                                 PreviewKeyDown="txtEmbarazo_PreviewKeyDown"/>


                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="4"
                                                   Text="PARTO"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,10"></TextBlock>
                                        <TextBox Grid.Row="0" 
                                                 Grid.Column="5"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 FontSize="16"
                                                 x:Name="txtParto"
                                                 PreviewKeyDown="txtParto_PreviewKeyDown"/>





                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="0"
                                                   Text="CESAREA"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,10"></TextBlock>
                                        <TextBox Grid.Row="1" 
                                                 Grid.Column="1"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 FontSize="16"
                                                 x:Name="txtCesarea"
                                                 PreviewKeyDown="txtCesarea_PreviewKeyDown"/>


                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="2"
                                                   Text="ABORTO"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,10"></TextBlock>
                                        <TextBox Grid.Row="1" 
                                                 Grid.Column="3"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 FontSize="16"
                                                 x:Name="txtAborto"
                                                 PreviewKeyDown="txtAborto_PreviewKeyDown"/>


                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="4"
                                                   Text="IVSA"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,10"></TextBlock>
                                        <TextBox Grid.Row="1" 
                                                 Grid.Column="5"
                                                 Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                 VerticalAlignment="Center"
                                                 Padding="0,5,0,5"
                                                 Margin="10,0,0,0"
                                                 FontSize="16"
                                                 x:Name="txtIVSA"
                                                 PreviewKeyDown="txtIVSA_PreviewKeyDown"/>

                                        <TextBlock Grid.Row="2"
                                                   Grid.Column="0"
                                                   Text="MPF"
                                                   VerticalAlignment="Bottom"
                                                   HorizontalAlignment="Right"
                                                   TextAlignment="Right"
                                                   TextWrapping="WrapWithOverflow"
                                                   Foreground="#9f9f9f"
                                                   Margin="5,5,5,7"></TextBlock>
                                        <ComboBox Grid.Row="2"
                                                  Grid.Column="1"
                                                  VerticalAlignment="Bottom"
